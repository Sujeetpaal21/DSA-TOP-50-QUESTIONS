//Question Link
//https://practice.geeksforgeeks.org/problems/insertion-sort/1?page=1&curated[]=8&sortBy=submissions

 void insertionSort(int arr[], int i)
    {
        
      for(int k=0;k<i;k++)
      {
          
          for(int j=k; j<i;j++)
          {
              if(arr[k]>arr[j])
              {
                  int temp=arr[k];
                      arr[k]=arr[j];
                      arr[j]=temp;
              }
          }
      }
    }
